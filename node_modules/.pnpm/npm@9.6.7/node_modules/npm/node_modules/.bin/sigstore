#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules/sigstore/bin/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules/sigstore/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/node_modules:/home/workspace/sample-project/myapp/node_modules:/home/workspace/sample-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules/sigstore/bin/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules/sigstore/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules/npm/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/npm@9.6.7/node_modules:/home/workspace/sample-project/myapp/node_modules/.pnpm/node_modules:/home/workspace/sample-project/myapp/node_modules:/home/workspace/sample-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sigstore/bin/sigstore.js" "$@"
else
  exec node  "$basedir/../sigstore/bin/sigstore.js" "$@"
fi
